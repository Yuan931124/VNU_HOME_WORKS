   "source": [
    "import random\n",
    "answer = random.sample(range(1, 10), 4)\n",
    "a = b = n = 0\n",
    "while a!=4:\n",
    "    a = b = n = 0\n",
    "    user = list(input('輸入四00個數字：'))\n",
    "    for i in user:\n",
    "        if int(user[n]) == answer[n]:\n",
    "            a += 1\n",
    "        else:\n",
    "            if int(i) in answer:\n",
    "                b += 1\n",
    "        n += 1\n",
    "    output = ','.join(user).replace(',','')\n",
    "    print(f'{output}: {a}A{b}B')\n",
    "print(\"答對了！\")"
   ]
